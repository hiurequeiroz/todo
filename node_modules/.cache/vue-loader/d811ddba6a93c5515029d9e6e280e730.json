n\n<script>\nimport Todo from './components/Todo'\n\nexport default {\n\tname: 'App',\n\tcomponents: { Todo },\n\tdata() {\n\t\treturn { todos: [], todo: { checked: false } };\n\t\t},\n\tmethods: {\n\t\taddTodo(todo) {\n\t\ttodo.id = Date.now();\n\t\tthis.todos.push(todo);\n\t\t//isso faz com que ele limpe a cada add\n\t\tthis.todo = {checked: false };\n\t\t}\n\t}\n};\n</script>\n \n<style scoped>\n\n .img-logo {\n max-width: 200px;\n margin: 0 auto;\n }\n\n .todo-lists {\n\tpadding-top: 2rem;\n }\n\n</style>\n"]}]}